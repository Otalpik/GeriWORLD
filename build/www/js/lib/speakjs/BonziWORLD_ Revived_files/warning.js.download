function hidewarning() {
  var x = document.getElementById("page_warning");
  if (x.style.display === "none") {
    x.style.display = "block";
  } else {
    x.style.display = "none";
  }
}
function hidemute() {
  var x = document.getElementById("page_mute");
  if (x.style.display === "none") {
    x.style.display = "block";
  } else {
    x.style.display = "none";
  }
}